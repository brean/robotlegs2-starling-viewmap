<flex-config xmlns="http://www.adobe.com/2006/flex-config">
  <compiler>
    <accessible>true</accessible>
    <fonts>
      <managers>
        <manager-class>flash.fonts.JREFontManager</manager-class>
        <manager-class>flash.fonts.BatikFontManager</manager-class>
        <manager-class>flash.fonts.AFEFontManager</manager-class>
        <manager-class>flash.fonts.CFFFontManager</manager-class>
      </managers>
    </fonts>
    <source-path>
      <path-element>src</path-element>
    </source-path>
    <locale>
      <locale-element>de_DE</locale-element>
    </locale>
    <theme>
      <filename>/opt/flex-sdk/frameworks/themes/Spark/spark.css</filename>
    </theme>
    <namespaces>
      <namespace>
        <uri>http://ns.adobe.com/mxml/2009</uri>
        <manifest>/opt/flex-sdk/frameworks/mxml-2009-manifest.xml</manifest>
      </namespace>
      <namespace>
        <uri>library://ns.adobe.com/flex/spark</uri>
        <manifest>/opt/flex-sdk/frameworks/spark-manifest.xml</manifest>
      </namespace>
      <namespace>
        <uri>library://ns.adobe.com/flex/mx</uri>
        <manifest>/opt/flex-sdk/frameworks/mx-manifest.xml</manifest>
      </namespace>
      <namespace>
        <uri>http://www.adobe.com/2006/mxml</uri>
        <manifest>/opt/flex-sdk/frameworks/mxml-manifest.xml</manifest>
      </namespace>
    </namespaces>
    <library-path>
      <path-element>/opt/flex-sdk/frameworks/locale/{locale}</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/rpc.swc</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/osmf.swc</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/spark.swc</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/charts.swc</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/framework.swc</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/spark_dmv.swc</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/sparkskins.swc</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/textLayout.swc</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/advancedgrids.swc</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/authoringsupport.swc</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/flash-integration.swc</path-element>
      <path-element>/opt/flex-sdk/frameworks/libs/mx/mx.swc</path-element>
    </library-path>
    <external-library-path>
      <path-element>/opt/flex-sdk/frameworks/libs/player/11.1/playerglobal.swc</path-element>
    </external-library-path>
  </compiler>
  <target-player>11.1</target-player>
  <swf-version>14</swf-version>
  <static-link-runtime-shared-libraries>false</static-link-runtime-shared-libraries>
  <runtime-shared-library-path>
    <path-element>/opt/flex-sdk/frameworks/libs/framework.swc</path-element>
